@model Reservation

<h2>Cập nhật Reservation</h2>

<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />

    <input asp-for="Name" /><br />
    <input asp-for="StartLocation" /><br />
    <input asp-for="EndLocation" /><br />

    <button type="submit">Update</button>
</form>
